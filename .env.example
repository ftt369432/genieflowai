# Example environment file - COPY TO .env.local AND ADD YOUR OWN VALUES
NODE_ENV=development
VITE_USE_MOCK=false

# Google OAuth Client ID for authentication
VITE_GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE
VITE_GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_HERE

# Google API Scopes
VITE_GOOGLE_SCOPES=https://www.googleapis.com/auth/gmail.readonly,https://www.googleapis.com/auth/gmail.modify,https://www.googleapis.com/auth/gmail.compose,https://www.googleapis.com/auth/gmail.send,https://mail.google.com/,https://www.googleapis.com/auth/userinfo.email,https://www.googleapis.com/auth/userinfo.profile,https://www.googleapis.com/auth/calendar.readonly,https://www.googleapis.com/auth/calendar.events.readonly

# Gemini API Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# AI Provider Configuration
VITE_AI_PROVIDER=google

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API Configuration for local development
VITE_API_URL=http://localhost:3000/api
VITE_AUTH_CALLBACK_URL=http://localhost:3000/auth/callback

# Google OAuth credentials
GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE
GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET_HERE
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/callback

# API endpoint (for client-side requests)
VITE_API_BASE_URL=https://api.example.com

# Other API Keys (if needed)
# VITE_OPENAI_API_KEY=your_openai_api_key_here
# VITE_GEMINI_API_KEY=your_gemini_api_key_here
# VITE_CLAUDE_API_KEY=your_claude_api_key_here

# Feature flags
VITE_ENABLE_CLAUDE=false
VITE_ENABLE_XAI=false
VITE_ENABLE_DEEPSEEK=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AI_FEATURES=true

# Service-specific configurations
VITE_AI_ASSISTANT_PROVIDER=gemini
VITE_AI_AGENT_PROVIDER=openai

# Knowledge Base configurations
VITE_EMBEDDING_MODEL=text-embedding-ada-002
VITE_EMBEDDING_PROVIDER=openai

# AI Service API Keys
ANTHROPIC_API_KEY=your_anthropic_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
OPENAI_API_KEY=your_openai_key_here

# Application Configuration
VITE_APP_NAME=GenieFlowAI
VITE_APP_URL=https://genieflowai.com

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Database (if needed)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=genieflow
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# Optional Services
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASSWORD=your_smtp_password

# API Keys
VITE_SERPAPI_KEY=your_serpapi_key_here
VITE_OPENAI_API_KEY=your_openai_key_here
VITE_ANTHROPIC_API_KEY=your_anthropic_key_here

# API Configuration
VITE_AUTH_CALLBACK_URL=http://localhost:3000/auth/callback

# Build Configuration
VITE_BUILD_MODE=development