# GenieFlowAI Environment Variables

# API URLs
VITE_API_URL=https://api.genieflowai.com
VITE_SERVER_URL=https://api.genieflowai.com
VITE_AUTH_CALLBACK_URL=https://genieflowai.com/auth/callback

# Authentication (Supabase)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google OAuth
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here
VITE_GOOGLE_API_KEY=your_google_api_key_here

# AI Services - Gemini Only
VITE_AI_ASSISTANT_PROVIDER=gemini
VITE_AI_AGENT_PROVIDER=gemini
VITE_EMBEDDING_MODEL=gemini-embedding-exp
VITE_EMBEDDING_PROVIDER=google
VITE_AI_MODEL=gemini-2.0-flash

# API Keys - Gemini Only
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Feature Flags
VITE_USE_MOCK=false
VITE_ENABLE_CLAUDE=false
VITE_ENABLE_XAI=false
VITE_ENABLE_DEEPSEEK=false
VITE_AI_PROVIDER=google

# Deployment
VITE_ENV=development
VITE_APP_NAME=GenieflowAI

# Application Settings
NODE_ENV=development
# Set to 'production' when deploying

# Other API Keys (if needed)
# VITE_OPENAI_API_KEY=your_openai_api_key_here
# VITE_GEMINI_API_KEY=your_gemini_api_key_here
# VITE_CLAUDE_API_KEY=your_claude_api_key_here

