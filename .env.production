# GenieFlowAI Production Environment Variables

# API URLs
VITE_API_URL=https://api.genieflowai.com
VITE_SERVER_URL=https://api.genieflowai.com
VITE_AUTH_CALLBACK_URL=https://genieflowai.com/auth/callback

# Authentication (Supabase) - These will be set in Netlify
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Google OAuth - These will be set in Netlify
VITE_GOOGLE_CLIENT_ID=
VITE_GOOGLE_API_KEY=

# Feature Flags
VITE_USE_MOCK=false
VITE_ENABLE_CLAUDE=false
VITE_ENABLE_XAI=false
VITE_ENABLE_DEEPSEEK=false

# AI Provider Configuration
VITE_AI_PROVIDER=google

# Service-specific configurations
VITE_AI_ASSISTANT_PROVIDER=gemini
VITE_AI_AGENT_PROVIDER=gemini

# Knowledge Base configurations
VITE_EMBEDDING_MODEL=gemini-embedding-exp
VITE_EMBEDDING_PROVIDER=google

# AI model configuration
VITE_AI_MODEL=gemini-2.0-flash

# Deployment
VITE_ENV=production
NODE_ENV=production

# Debug settings
VITE_DEBUG_MODE=false 