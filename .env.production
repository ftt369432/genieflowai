# Google OAuth Credentials
VITE_GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID
VITE_GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET

# API Configuration
VITE_API_URL=https://api.genieflowai.com
VITE_AUTH_CALLBACK_URL=https://genieflowai.com/auth/callback

# Environment
NODE_ENV=production
VITE_USE_MOCK=false

# AI Provider Configuration
VITE_AI_PROVIDER=google
VITE_AI_MODEL=gemini-2.0-flash

# Feature flags - disable all other providers
VITE_ENABLE_CLAUDE=false
VITE_ENABLE_XAI=false
VITE_ENABLE_DEEPSEEK=false

# Service-specific configurations
VITE_AI_ASSISTANT_PROVIDER=google
VITE_AI_AGENT_PROVIDER=google

# Knowledge Base configurations
VITE_EMBEDDING_MODEL=embedding-001
VITE_EMBEDDING_PROVIDER=google

# Debug settings
VITE_DEBUG_MODE=false

# Model configuration
VITE_MAX_TOKENS=10000
VITE_TEMPERATURE=0.7
VITE_TOP_P=0.95
VITE_TOP_K=40
VITE_MAX_RETRIES=3
VITE_RETRY_DELAY=1000
VITE_TIMEOUT=30000