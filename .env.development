VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_GEMINI_API_KEY=your_gemini_key_here
VITE_CLAUDE_API_KEY=your_claude_key_here
VITE_XAI_API_KEY=your_xai_key_here
VITE_DEEPSEEK_API_KEY=your_deepseek_key_here

# Feature flags - disable all other providers
VITE_ENABLE_CLAUDE=false
VITE_ENABLE_XAI=false
VITE_ENABLE_DEEPSEEK=false

# Service-specific configurations
VITE_AI_ASSISTANT_PROVIDER=google
VITE_AI_AGENT_PROVIDER=google

# Knowledge Base configurations
VITE_EMBEDDING_MODEL=gemini-embedding-exp
VITE_EMBEDDING_PROVIDER=google

# AI model configuration
VITE_AI_MODEL=gemini-2.0-flash

# Disable Fast Refresh to fix build issues
FAST_REFRESH=false
DISABLE_ESLINT_PLUGIN=true 